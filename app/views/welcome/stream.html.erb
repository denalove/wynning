  <div id="stream_heading">
    <h1>Wynner's Circle</h1>
  </div>
    <% @stream.each do |key, value| %>
    <div class="col-lg-3"></div>
    <div class="container">
    <div class="col-lg-8">
        <div class="panel panel-primary">
            <div class="panel-heading">
                <div class="row">
                    <div class="col-xs-1">
                        <i class="fa fa-bullhorn fa-5x"></i>
                    </div>
                    <div class="col-xs-9 text-left stream_content">
                        <div class="huge" style="font-size:35px"><%= value[1].first_name %> <%= value[1].last_name %> </div>
                        <div style="font-size:25px">Completed: <%= value[0].title %> <%= time_ago_in_words(value[0].updated_at)%> ago!</div>
                    </div>
                </div>
            </div>
        </div>
      </div>
    </div>
  <% end %>

