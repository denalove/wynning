	<!-- multistep form -->
<form id="msform">
	<!-- progressbar -->
	<ul id="progressbar">
		<li class="active">Account Setup</li>
		<li>Social Profiles</li>
		<li>Personal Details</li>
	</ul>
	<!-- fieldsets -->
	<fieldset>
		<!-- <h2 class="fs-title">What is your Dream?</h2> -->
		<input type="text" name="dream" placeholder="What is your Dream?" />
		<h2 class="fs-subtitle">By when do you want to realize your dream?</h2>
		<input type="date" name="dreamDate" placeholder="When do you want to fulfill your Dream?" />
		<!-- <input type="password" name="cpass" placeholder="Confirm Password" /> -->
		<input type="button" name="next" class="next action-button" value="Next" />
	</fieldset>
	<fieldset>
		<h2 class="fs-title">One Month Goal</h2>
		<h3 class="fs-subtitle"></h3>
		<input type="text" name="One Month Goal" placeholder="What is your One Month Goal?" />
		<input type="date" name="1monthgoal" placeholder="When do you want to acheive your One Month Goal?" />
		<!-- <input type="text" name="gplus" placeholder="Google Plus" /> -->
		<input type="button" name="previous" class="previous action-button" value="Previous" />
		<input type="button" name="next" class="next action-button" value="Next" />
	</fieldset>
	<fieldset>
		<h2 class="fs-title">Activity</h2>
		<h3 class="fs-subtitle"></h3>
		<input type="text" name="Activity" placeholder="What is something your can do to reach your One Month Goal?" />
		<input type="date" name="1monthgoal" placeholder="When do you want to acheive your One Month Goal?" />
		<!-- <input type="text" name="gplus" placeholder="Google Plus" /> -->
		<input type="button" name="previous" class="previous action-button" value="Previous" />
		<input type="button" name="next" class="next action-button" value="Next" />
	</fieldset>
	<fieldset>
		<h2 class="fs-title">Personal Details</h2>
		<h3 class="fs-subtitle">Let us know who you are!</h3>
		<input type="text" name="fname" placeholder="First Name" />
		<input type="text" name="lname" placeholder="Last Name" />
		<input type="text" name="phone" placeholder="Phone" />
		<input type="email" name="email" placeholder="E-Mail"/>
		<input type="button" name="previous" class="previous action-button" value="Previous" />
		<input type="submit" name="submit" class="submit action-button" value="Submit" />
	</fieldset>
</form>
