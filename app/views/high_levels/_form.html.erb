<h2>Enter your new short term goal here!</h2>
<%= form_for([@user, @high_level]) do |f| %>
    <div class="form group">
      <%= f.label :title %><br>
      <span id="HLGtitle"><%= f.text_field :title %></span>
    </div>
    <div class="form group">
      <%= f.label :due %><br>
      <%= f.text_field :due, :as => :dateselect %>
    </div>

    <div class="actions" id="addHLG">
      <%= f.submit 'Add', :class => 'btn btn-success' %>
    </div>

<% end %>
</form>
